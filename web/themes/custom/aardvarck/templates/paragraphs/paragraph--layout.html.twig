{#
/**
 * @file
 * Theme implementation to display a layout paragraph.
 */
#}
{%
  set classes = [
    'section--paragraph-layout',
  ]
%}
{% block paragraph %}
  {% embed 'aardvarck:section' with {
    variant: custom_values.background_style,
    attributes: attributes.addClass(classes),
    content: content
    } only %}
  {% endembed %}
{% endblock paragraph %}
